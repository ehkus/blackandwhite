{
    "name": "Black and White",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Makes all background colors white and all text black",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "web_accessible_resources": [
	"blackandwhite.css"
    ],
    "permissions": [
	"activeTab",
	"storage"
    ]
}
